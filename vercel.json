{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/portal",
      "dest": "/server.js"
    },
    {
      "src": "/config",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "",
      "USER_USERNAME": "",
      "USER_PASSWORD": "",
      "GOOGLE_API_KEY": "",
      "MISTRAL_API_KEY": "",
      "CLAUDE_API_KEY": ""
  },
  "build": {
    "env": {
    }
  }
}
